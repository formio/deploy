{
    "compose": {
        "aws": {
            "api-server.zip": {
                "mongoCert": "https://s3.amazonaws.com/rds-downloads/rds-combined-ca-bundle.pem",
                "pdf": false,
                "aws": true
            },
            "pdf-server.zip": {
                "mongoCert": "https://s3.amazonaws.com/rds-downloads/rds-combined-ca-bundle.pem",
                "server": false,
                "aws": true
            },
            "uswds-viewer.zip": {
                "pdf": "formio/uswds-viewer",
                "mongoCert": "https://s3.amazonaws.com/rds-downloads/rds-combined-ca-bundle.pem",
                "pdfVersion": "<%- options.uswdsVersion %>",
                "aws": true,
                "nginx": true
            },
            "multicontainer.zip": {
                "mongoCert": "https://s3.amazonaws.com/rds-downloads/rds-combined-ca-bundle.pem",
                "aws": true,
                "nginx": true
            },
            "multicontainer.gov.zip": {
                "mongoCert": "https://truststore.pki.us-gov-west-1.rds.amazonaws.com/global/global-bundle.pem",
                "mongoCertName": "rds-combined-ca-us-gov-bundle.pem",
                "aws": true,
                "nginx": true
            },
            "multicontainer.gov.ssl.zip": {
                "mongoCert": "https://truststore.pki.us-gov-west-1.rds.amazonaws.com/global/global-bundle.pem",
                "mongoCertName": "rds-combined-ca-us-gov-bundle.pem",
                "ssl": true,
                "aws": true,
                "nginx": true
            }
        },
        "local.zip": {
            "local": true,
            "mongo": "mongodb://mongo:27017/formio",
            "nginx": true
        },
        "multicontainer.zip": {
            "local": true,
            "mongo": "mongodb://mongo:27017/formio",
            "includeEnv": true,
            "nginx": true
        },
        "api-server.zip": {
            "local": true,
            "mongo": "mongodb://mongo:27017/formio",
            "pdf": false,
            "includeEnv": true
        },
        "pdf-server.zip": {
            "local": true,
            "mongo": "mongodb://mongo:27017/formio",
            "server": false,
            "includeEnv": true
        }
    }
}